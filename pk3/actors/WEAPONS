actor DukeWeapon0Spawner : CustomInventory
{
  -INVENTORY.ALWAYSPICKUP
  -COUNTITEM
  +DONTGIB
  Inventory.PickUpSound "duke/itemget"
  Inventory.PickupMessage "You got the Pipebombs!"
  Scale 0.5
  States
  {
  Spawn:
    WPBM A -1
    stop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("Pipebombs",1,"PickupFailCheck")
    TNT1 A 0 A_Print("You got the Pipebombs!")
	TNT1 A 0 A_JumpIfInventory("DukeTauntCooldown",1,"Pickup2")
    TNT1 A 0 A_JumpIfInventory("DukeBallgag",1,"Pickup2")
	TNT1 AA 0
    TNT1 A 0 A_PlaySoundEx("duke/weapontaunt","soundslot7",0,0)
	TNT1 A 0 A_GiveInventory("DukeTauntCooldown",5)
	TNT1 A 0 ACS_ExecuteAlways(205)
  Pickup2:
    TNT1 A 0 A_GiveInventory("GotWeapon0",1)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 1 A_GiveInventory("Pipebombs")
    stop
  PickupFailCheck:
	TNT1 A 0 A_JumpIfInventory("RocketAmmo",0,"PickupFail")
	TNT1 A 0 A_JumpIfInventory("DukeTauntCooldown",1,"Pickup2")
    TNT1 A 0 A_JumpIfInventory("DukeBallgag",1,"Pickup2")
	TNT1 AA 0
    TNT1 A 0 A_PlaySoundEx("duke/weapontaunt","soundslot7",0,0)
	TNT1 A 0 A_GiveInventory("DukeTauntCooldown",5)
	TNT1 A 0 ACS_ExecuteAlways(205)
	goto Pickup2
  PickupFail:
	TNT1 A 0
	fail
  }
}

actor MarathonWeapon0Spawner : CustomInventory
{
  -INVENTORY.ALWAYSPICKUP
  -COUNTITEM
  +DONTGIB
  Inventory.PickUpSound "marathon/itemget"
  Inventory.PickupMessage "You got the KKV-7 SMG Flechette!"
  Scale 0.35
  States
  {
  Spawn:
    FSMG A -1
    stop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("KKV-7 SMG Flechette",1,"PickupFailCheck")
    TNT1 A 0 A_Print("You got the KKV-7 SMG Flechette!")
    TNT1 A 0 A_GiveInventory("GotWeapon0",1)
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 A 0 A_GiveInventory("Clip",20)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 1 A_GiveInventory("KKV-7 SMG Flechette")
    stop
  PickupFailCheck:
	TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 1 A_GiveInventory("Clip",10)
    stop
  PickupFail:
	TNT1 A 0
	fail
  }
}

actor MarathonWeapon0Spawner2 : CustomInventory
{
  -INVENTORY.ALWAYSPICKUP
  -COUNTITEM
  +DONTGIB
  Inventory.PickUpSound "marathon/itemget"
  Inventory.PickupMessage "You got a second .44 Magnum Mega Class A1!"
  Scale 0.35
  States
  {
  Spawn:
    PSTL A -1
    stop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("CanDualPistols",0,"PickupFailCheck")
    TNT1 A 0 A_Print("You got a second .44 Magnum Mega Class A1!")
    TNT1 A 0 A_GiveInventory("CanDualPistols",1)
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 A 0 A_GiveInventory("Clip",10)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    stop
  PickupFailCheck:
	TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
