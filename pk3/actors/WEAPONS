actor DukeWeapon2Spawner : CustomInventory
{
  -COUNTITEM
  +DONTGIB
  Inventory.PickUpSound "duke/mosspump2"
  Inventory.PickupMessage "You got the Shotgun!"
  Scale 0.5
  States
  {
  Spawn:
    SHTP A -1
    stop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("  Shotgun  ",1,"PickupFailCheck")
    TNT1 A 0 A_Print("You got the Shotgun!")
	TNT1 A 0 A_JumpIfInventory("DukeTauntCooldown",1,"Pickup2")
    TNT1 A 0 A_JumpIfInventory("DukeBallgag",1,"Pickup2")
	TNT1 AA 0
    TNT1 A 0 A_PlaySoundEx("duke/weapontaunt","soundslot7",0,0)
	TNT1 A 0 A_GiveInventory("DukeTauntCooldown",5)
	TNT1 A 0 ACS_ExecuteAlways(205)
  Pickup2:
    TNT1 A 0 A_GiveInventory("GotWeapon2",1)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 1 A_GiveInventory("  Shotgun  ")
    stop
  PickupFailCheck:
	TNT1 A 0 A_JumpIfInventory("AmmoShell",0,"PickupFail")
	TNT1 A 0 A_JumpIfInventory("DukeTauntCooldown",1,"Pickup2")
    TNT1 A 0 A_JumpIfInventory("DukeBallgag",1,"Pickup2")
	TNT1 AA 0
    TNT1 A 0 A_PlaySoundEx("duke/weapontaunt","soundslot7",0,0)
	TNT1 A 0 A_GiveInventory("DukeTauntCooldown",5)
	TNT1 A 0 ACS_ExecuteAlways(205)
	goto Pickup2
  PickupFail:
	TNT1 A 0
	fail
  }
}

actor MarathonWeapon2Spawner : CustomInventory
{
  -INVENTORY.ALWAYSPICKUP
  -COUNTITEM
  +DONTGIB
  Inventory.PickUpSound "marathon/itemget"
  Inventory.PickupMessage "You got the WSTE-M5 Combat Shotgun!"
  Scale 0.35
  States
  {
  Spawn:
    SHTY A -1
    stop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("WSTE-M5 Combat Shotgun",1,"PickupFailCheck")
    TNT1 A 0 A_Print("You got the WSTE-M5 Combat Shotgun!")
    TNT1 A 0 A_GiveInventory("GotWeapon2",1)
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 0 A_GiveInventory("AmmoShell",8)
    TNT1 A 1 A_GiveInventory("WSTE-M5 Combat Shotgun")
    stop
  PickupFailCheck:
	TNT1 A 0 A_JumpIfInventory("CanDualShotties",1,"PickupFailCheck2")
	TNT1 A 0 A_Print("You got a second WSTE-M5 Combat Shotgun!")
	TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
	TNT1 A 0 A_GiveInventory("CanDualShotties",1)
	TNT1 A 0 A_SetBlend("Green",0.4,4)
	WEPG B 1 A_GiveInventory("AmmoShell",8)
    stop
  PickupFailCheck2:
	TNT1 A 0 A_JumpIfInventory("AmmoShell",0,"PickupFail")
	TNT1 A 0 A_SetBlend("Green",0.4,4)
    TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("WeaponSparkle",0,0,random(4,52),random(-2,2),random(-2,2),random(1,3),48,128)
    TNT1 A 1 A_GiveInventory("AmmoShell",8)
    stop
  PickupFail:
	TNT1 A 0
	fail
  }
}


