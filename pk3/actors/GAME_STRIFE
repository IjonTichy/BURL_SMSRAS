        ROB1 H 0 A_GenericFreezeDeath
        ROB1 H 70 A_UnsetSolid
        ROB1 H 0 A_IceGuyDie
        ROB1 H 1 A_IceGuyDie
        TNT1 A -1
        stop
    }
